<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mainicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PlaylistPlaying"
            android:label="@string/playlist_playing">

        </activity>
        <activity
            android:name=".PodCastPlaying"
            android:label="@string/title_activity_playing_pod_cast">

        </activity>
        <activity
            android:name=".NowPlaying"
            android:label="Now playing...." />
        <activity
            android:name=".PodCasts"
            android:label="@string/albums" />
        <activity
            android:name=".LikedTracks"
            android:label="@string/liked_tracks" />
        <activity
            android:name=".Playlists"
            android:label="@string/playlist" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>